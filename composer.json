{
    "name": "ogigunes/tron-api",
    "description": "A PHP API for interacting with Tron (TRX)",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/ogigunes/tron-api.git",
    "authors": [
        {
            "name": "ogi",
            "email": "ogingunes@gmail.com"
        }
    ],
    "keywords": [
        "tron-lib",
        "tron-php",
        "tron-api",
        "tron-rest-api"
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.2",
        "phpseclib/phpseclib": "~2.0",
        "web3p/web3.php": "^0.1.6",
        "kornrunner/secp256k1": "^0.2",
        "simplito/elliptic-php": "^1.0",
        "ext-json": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "OgiGunes\\TronAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OgiGunes\\TronAPI\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
